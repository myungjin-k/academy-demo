{{>layout/header}}
<body style="width: 100%; height: 100%;">
{{>layout/navigation}}
<div class="container-fluid" id="container-content">
    <div class="row">
        <!-- Content -->
        {{>layout/category}}
        <div class="col-sm-8" id="root" style="overflow: auto;">
            <!-- Index Content -->
            <!-- Item List -->
            <div id="div-sales-item-list" class="contentDiv">
                <div class="categoryTitle">
                    <p id="title">
                        <a class="main text-decoration-none">BEST</a>
                    </p>
                    <div id="div-sub-cates"></div>
                </div>
                <div class="row" id="div-thumb">

                </div>
                <div id="div-load-more-items"></div>
            </div>
            <!-- //Item List -->
            <!-- Item Detail -->
            <div class="d-none p-2 contentDiv" id="div-sales-item-detail">
                <div>
                    <p id="breadcrumb">
                        <a class="categoryTitle all" href="" onclick="return false;">
                            All
                        </a>
                        <span class="cateArea main">
                            >
                            <a class="categoryTitle" href="" onclick="return false;">
                                Main Category
                            </a>
                            <input type="hidden" name="categoryId" />
                        </span>
                        <span class="cateArea sub d-none">
                            >
                            <a class="categoryTitle" href="" onclick="return false;">
                                Sub Category
                            </a>
                            <input type="hidden" name="categoryId" />
                        </span>
                    </p>

                </div>
                <div class="row text-center">
                    <div class="col-sm-9">
                        <div class="row mb-lg-5">
                            <div class="text-center" id="div-item-detail-thumbnail">
                                <img src="" id="img-thumbnail" />
                            </div>
                        </div>
                        <ul class="nav nav-tabs" id="itemDetailTab" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" id="tab-item-info" data-toggle="tab" href="#div-item-info" role="tab" aria-controls="div-item-info" aria-selected="true">상세정보</a>
                            </li>
                            <li class="nav-item" >
                                <a class="nav-link" id="tab-item-reivew" data-toggle="tab" href="#div-item-review" role="tab" aria-controls="div-item-review" aria-selected="false">리뷰</a>
                            </li>
                        </ul>
                        <div class="tab-content" id="itemDetailTabContent">
                            <div class="tab-pane fade show active" id="div-item-info" role="tabpanel" aria-labelledby="tab-item-info" >
                                <div class="row m-lg-5">
                                    <div id="div-item-detail-notice" style="width: 100%;">
                                        <p class="text-center" id="p-notice" ></p>
                                    </div>
                                </div>
                                <div class="row mb-5">
                                    <div id="div-item-detail-description" >
                                        <textarea class="text-center border-0" id="text-description" readonly></textarea>
                                    </div>
                                </div>
                                <div class="row mb-lg-5">
                                    <div class="text-center" id="div-item-detail-image">
                                        <img src="" id="img-detail"/>
                                    </div>
                                </div>
                            </div>
                            <!-- reviews -->
                            <div class="tab-pane fade" id="div-item-review" role="tabpanel" aria-labelledby="tab-item-review" >
