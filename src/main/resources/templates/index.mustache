                        <thead class="thead-strong">
                        <tr>
                            <th width="3%">선택</th>
                            <th >상품명</th>
                            <th width="10%">수량</th>
                            <th width="10% ">금액</th>
                            <th width="15%"></th>
                        </tr>
                        </thead>
                        <tbody id="cart-items">

                        </tbody>
                    </table>
                    <a class="btn btn-lg btn-primary" id="btn-order">주문하기</a>
                </div>
            </div>
            <!-- // Cart -->
            <!-- Order -->
            <div class="d-none p-2 contentDiv" id="div-order" >
